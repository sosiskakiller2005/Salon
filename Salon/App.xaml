<Application x:Class="Salon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Salon"
             xmlns:Salon="clr-namespace:DateTimePickerCustom;assembly=DateTimePickerCustom"
             StartupUri="\Views\Windows\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="\Styles\ButtonStyle.xaml"/>
                <ResourceDictionary Source="\Styles\TextBlockStyle.xaml"/>
                <ResourceDictionary Source="\Styles\TextBoxStyle.xaml"/>
                <ResourceDictionary Source="\Styles\ListBoxStyle.xaml"/>
                <ResourceDictionary Source="\Styles\ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="\Styles\DatePicker.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
